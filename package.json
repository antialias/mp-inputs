{
  "name": "insights",
  "version": "1.0.0",
  "description": "Mixpanel's Insights report",
  "main": "index.js",
  "scripts": {
    "benchmark": "babel-node --presets es2015 --plugins syntax-async-functions,transform-regenerator benchmark/run",
    "build": "webpack",
    "build-test": "webpack --config test/browser/webpack.config.js",
    "dev-server": "webpack-dev-server --inline --colors",
    "lint": "eslint src",
    "production-build": "rm -rf build-production && NODE_ENV=production webpack",
    "standalone": "webpack-dashboard -- webpack-dev-server --inline --colors",
    "start": "npm run watch",
    "test": "npm run test-unit && npm run build-test && npm run test-browser-sauce",
    "test-browser-local": "wct --plugin local test/browser/index.html",
    "test-browser-sauce": "wct --plugin sauce test/browser/index.html",
    "test-p": "wct --plugin local --persistent test/browser/index.html",
    "test-unit": "mocha --require babel-core/register --require babel-polyfill test/unit",
    "watch": "webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mixpanel-platform/insights.git"
  },
  "keywords": [
    "mixpanel"
  ],
  "author": "dev@mixpanel.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixpanel-platform/insights/issues"
  },
  "homepage": "https://github.com/mixpanel-platform/insights#readme",
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "d3-dsv": "1.0.3",
    "framesg": "0.1.2",
    "jsurl": "0.1.5",
    "lodash": "4.15.0",
    "mixpanel-common": "0.73.0",
    "moment": "2.13.0",
    "panel": "0.10.0-rc9",
    "pikaday": "git://github.com/dbushell/Pikaday.git#a1704adef06e985a448cd8ab313dee5a5787a228",
    "webcomponent": "0.1.2",
    "webcomponents.js": "0.7.22"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer-loader": "3.1.0",
    "babel-cli": "6.10.1",
    "babel-core": "6.3.17",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.0",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-regenerator": "6.22.0",
    "babel-preset-es2015": "6.3.13",
    "css-loader": "0.23.1",
    "eslint": "3.4.0",
    "eslint-config-mixpanel": "2.3.0",
    "eslint-loader": "1.5.0",
    "expect.js": "0.3.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.15.0",
    "js-beautify": "1.5.10",
    "mixpanel": "0.4.0",
    "mocha": "2.5.3",
    "node-fetch": "1.5.3",
    "style-loader": "0.13.0",
    "stylus": "0.53.0",
    "stylus-loader": "2.4.0",
    "url-loader": "0.5.7",
    "virtual-jade": "0.8.2",
    "virtual-jade-loader": "0.5.0",
    "web-component-tester": "5.0.0",
    "webpack": "1.12.9",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-server": "1.14.1",
    "whatwg-fetch": "0.11.0"
  }
}
