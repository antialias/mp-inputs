{
  "name": "irb",
  "version": "0.1.0",
  "description": "Interactive report builder",
  "main": "index.js",
  "scripts": {
    "deploy": "git checkout master && npm run production-build && git add . && git commit -m rebuild && git push origin master",
    "lint": "eslint src",
    "production-build": "rm -rf dist && NODE_ENV=production ./node_modules/.bin/webpack",
    "revert": "git checkout master && git revert && git push origin master",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --progress --colors --watch",
    "serve": "./node_modules/.bin/webpack-dev-server --inline",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mixpanel-platform/irb.git"
  },
  "keywords": [
    "mixpanel"
  ],
  "author": "dev@mixpanel.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/irb/irb/issues"
  },
  "homepage": "https://github.com/irb/irb#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "framesg": "^0.1.2",
    "lodash": "^4.7.0",
    "mixpanel-browser": "^2.7.2",
    "panel": "^0.4.1",
    "rollbar-browser": "^1.8.5",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.17",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "js-beautify": "^1.5.10",
    "style-loader": "^0.13.0",
    "stylus": "^0.53.0",
    "stylus-loader": "^1.6.1",
    "url-loader": "^0.5.7",
    "virtual-jade": "^0.2.2",
    "virtual-jade-loader": "^0.2.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  }
}
