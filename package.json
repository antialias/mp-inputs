  "license": "MIT",
  "bugs": {
    "url": "https://github.com/irb/irb/issues"
  },
  "homepage": "https://github.com/irb/irb#readme",
  "dependencies": {
    "babel-polyfill": "6.7.4",
    "framesg": "0.1.2",
    "lodash": "4.15.0",
    "mixpanel-common": "0.26.4",
    "moment": "2.13.0",
    "panel": "0.9.1",
    "pikaday": "1.5.1",
    "virtual-dom": "2.1.1",
    "webcomponent": "0.1.2",
    "webcomponents.js": "0.7.22"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.1.0",
    "babel-cli": "6.10.1",
    "babel-core": "6.3.17",
