    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.15.0",
    "js-beautify": "1.5.10",
    "mixpanel": "0.4.0",
    "mocha": "2.5.3",
    "node-fetch": "1.5.3",
