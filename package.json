{
  "name": "irb",
  "version": "0.1.0",
  "description": "Interactive report builder",
  "main": "index.js",
  "scripts": {
    "benchmark": "babel-node --presets es2015 --plugins syntax-async-functions,transform-regenerator benchmark/run",
    "build": "webpack",
    "lint": "eslint src",
    "production-build": "rm -rf build-production && NODE_ENV=production webpack",
    "serve": "webpack-dev-server --inline",
    "start": "npm run watch",
    "test": "mocha --require babel-core/register --require babel-polyfill",
    "watch": "webpack --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mixpanel-platform/irb.git"
  },
  "keywords": [
    "mixpanel"
  ],
