{
  "rules": {
    "comma-dangle":
      [2, "always-multiline"],
    "eol-last":
      2,
    "eqeqeq":
      2,
    "indent":
      [2, 2, {"SwitchCase": 1}],
    "linebreak-style":
      [2, "unix"],
    "no-console":
      [1, {"allow": ["info", "warn", "error"]}],
    "no-debugger":
      1,
    "no-unused-expressions":
      2,
    "quotes":
      [2, "single"],
    "semi":
      [2, "always"]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
